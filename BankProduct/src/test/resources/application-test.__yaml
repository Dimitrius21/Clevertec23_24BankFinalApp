spring:
  datasource:
    username: postgres
    password: 1234
  jpa:
    hibernate:
      ddl-auto: none
    show-sql: true
    properties:
      hibernate:
        format_sql: true
    open-in-view: false
  data:
    web:
      pageable:
        default-page-size: 20
        max-page-size: 40
  liquibase:
    change-log: db/changelog/db.changelog-test.yaml
exception:
  custom-handler:
    enable: true
